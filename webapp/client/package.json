  "scripts": {
    "start": "cross-env PORT=3001 BROWSER=none EXTEND_ESLINT=true react-app-rewired start",
    "build": "cross-env EXTEND_ESLINT=true react-app-rewired build",
    "test": "cross-env react-app-rewired test --modulePaths=src",
    "test:CI": "cross-env CI=true react-app-rewired test --modulePaths=src --no-watch",
    "test:debug": "cross-env DEBUG=jest react-app-rewired test --modulePaths=src",
    "lint": "eslint src --no-error-on-unmatched-pattern --ext .ts,.tsx,.js,.jsx",
    "eject": "react-scripts eject",
    "pretty": "prettier --config ../.prettierrc.json --write \"src/**/*.{ts*,css,js*}\"",
    "analyze": "source-map-explorer build/static/js/main.*",
    "lint-fix": "eslint src --no-error-on-unmatched-pattern --ext .ts,.tsx,.js,.jsx --fix-dry-run ",
    "check-types": "node_modules/.bin/tsc -p . --noEmit",
    "graphql:download-schema": "cd graphqlScripts && sh download_graphql_schema.sh",
    "graphql:validate-queries": "cd graphqlScripts && sh validate_graphql_queries.sh",
    "graphql:update-types": "cd graphqlScripts && sh update_graphql_types.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/d3": "^5.7.1",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.122",
    "@types/moment": "^2.13.0",
    "@types/node": "^11.10.4",
    "@types/query-string": "^6.3.0",
    "@types/ramda": "^0.26.6",
    "@types/rc-tooltip": "^3.7.2",
    "@types/react": "^16.9.17",
    "@types/react-codemirror": "^1.0.2",
    "@types/react-custom-scrollbars": "^4.0.5",
    "@types/react-dom": "^16.9.4",
    "@types/react-modal": "^3.8.1",
    "@types/react-onclickoutside": "^6.7.0",
    "@types/react-paginate": "^6.2.1",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/uuid": "^3.4.4",
    "axios-mock-adapter": "^1.16.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.3.5",
    "husky": "^1.3.1",
    "jest-canvas-mock": "^2.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "source-map-explorer": "^1.7.0",
    "ts-graphql-plugin": "^1.11.0",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0"
  },
  "hooks": {
    "pre-commit": "yarn check-types && lint-staged",
    "pre-push": "yarn check-types && yarn test:CI"
  },
  "proxy": "http://localhost:3000",
  "lint-staged": {
    "src/**/*.{ts*,css,js*}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "mocks",
      "__tests__/mock.tsx?",
      "__tests__/fixtures?.tsx?",
      "__tests__/helpers.tsx?",
      "__tests__/queries.tsx?"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ]
  }
}
